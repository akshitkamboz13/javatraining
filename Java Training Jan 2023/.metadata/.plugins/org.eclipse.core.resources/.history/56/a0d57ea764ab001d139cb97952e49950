package Java.Training.OOPS.feb9;
// 0 1 1 2 3 5 8 13......
public class fibonacci
{
    int fib (int x)
    {
        if (x == 0)
        {
            return 0;
        }
        else if (x == 1)
        {
            return 1;
        }
        else
        {
            return fib(x-1) + fib(x-2);
        }

    }

    public static void main(String[] args)
    {
        fibonacci obj = new fibonacci();
        for (int i = 0; i <=10; i++)
        {
            int ans = obj.fib(i);
            System.out.println(ans);
        }

    }
}
