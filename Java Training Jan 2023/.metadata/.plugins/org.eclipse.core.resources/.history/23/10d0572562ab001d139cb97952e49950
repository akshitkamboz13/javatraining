package Java.Training.OOPS.feb2;

public class string
{
    void fun(String strr)
    {
        for (int i = 0; i<strr.length(); i++)
        {
            System.out.println(strr); // string sara print hoga
            System.out.println(strr.length()); // string ki length print hogi
            System.out.println(strr.charAt(i)); // string k har index k elements alah alag print honge
        }
    }
    public static void main(java.lang.String[] args)
    {
        java.lang.String str = "hello";

        string obj = new string();
        obj.fun(str);
    }
}
