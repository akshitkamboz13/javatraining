package Java.Training.OOPS.a_jan;

public class Training16_callby_value
{
    /*
    void fun(int x)
    {
        System.out.println("X = " +x);
    }
    public static void main(String[] args)
    {
        int num = 5;
        Training16_callby_value obj = new Training16_callby_value();
        obj.fun(num);
    }
    */
    void fun(int x, int y, String z, double a, float b)
    {
        System.out.println("X = " +x);
        System.out.println("y = " +y);
        System.out.println("z = " +z);
        System.out.println("a = " +a);
        System.out.println("b = " +b);
    }
    public static void main(String[] args)
    {
        int num = 5;
        Training16_callby_value obj = new Training16_callby_value();
        obj.fun(num, 7, "hello",2.6, 2.5f);
    }

}
