package Java.Training.OOPS.a_jan;

// Interface class contains undefined functions and the class that implements it should define all those undefined function.

interface pp
{
    void fun();
}

public class Training29_Interface implements pp
{
    public void fun()
    {
        System.out.println("hello");
    }
    public static void main(String[] args)
    {
        Training29_Interface obj = new Training29_Interface();
    }
}
