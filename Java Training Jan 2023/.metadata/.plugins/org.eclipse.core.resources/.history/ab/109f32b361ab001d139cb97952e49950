package Java.Training.OOPS.feb1;
//constructor chaining
class AA
{
    AA()
    {
        System.out.println("i am in AA");
    }

}
class BB extends AA
{
    BB()
    {
        System.out.println("i am in BB");
    }

}
public class T1default_constructor_chaining extends BB
{
    T1default_constructor_chaining()
    {
        System.out.println("i am in main");
    }

    public static void main(String[] args)
    {
        T1default_constructor_chaining obj = new T1default_constructor_chaining();
    }
}
