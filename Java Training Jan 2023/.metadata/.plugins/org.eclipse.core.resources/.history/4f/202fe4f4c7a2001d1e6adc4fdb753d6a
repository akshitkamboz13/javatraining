package unknown;

public class uptolow {
	void fun(String str) {
		String gtr=str.toUpperCase();
		String ptr=str.toLowerCase();
		String olla="";
		for(int i=0;i<str.length();i++) {
			char suui =str.charAt(i);
			if(str.charAt(i)!=gtr.charAt(i)) {
				suui =gtr.charAt(i);
			}
			else {
				suui =ptr.charAt(i);
			}
			System.out.println(suui);
			olla=olla+suui;
		}
		System.out.println(olla);
	}
	public static void main(String[] args) {
		String str="HellO";
		uptolow obj=new uptolow();
		
	}

}
